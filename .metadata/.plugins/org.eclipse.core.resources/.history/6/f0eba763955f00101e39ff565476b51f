package network;

import domain.DeviceType;

public class ClientInformation {

    private RMIClientInterface stub;
    private String ipAddress;
    DeviceType deviceType;

    public ClientInformation(RMIClientInterface stub, String host) {
	this.stub = stub;
	this.ipAddress = host;
    }

    public ClientInformation(RMIClientInterface client, String host, DeviceType deviceType) {
	this.stub = stub;
	this.ipAddress = host;
	this.deviceType = deviceType;
    }

    public String getHost() {
	return ipAddress;
    }

    public RMIClientInterface getStub() {
	return stub;
    }

}
